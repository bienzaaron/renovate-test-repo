{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "baseBranches": [
    "main"
  ],
  "packageRules": [
    {
      "matchDatasources": [
        "node-version"
      ],
      "registryUrls": [
        "http://mynodejsmirror.com:3000/some/dir"
      ]
    },
    {
      "matchDatasources": [
        "npm"
      ],
      "registryUrls": [
        "http://mynpmregistry.com:3000/some/other/dir/"
      ]
    }
  ]
}